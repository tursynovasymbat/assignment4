package com.example.school.db;

import java.sql.Connection;
import java.sql.DriverManager;

public class DB {

    public static Connection connect() {
        try {
            return DriverManager.getConnection(
                    "jdbc:postgresql://localhost:5432/postgres",
                    "postgres",
                    "123456"
            );
        } catch (Exception e) {
            e.printStackTrace();
            return null;
        }
    }
}
